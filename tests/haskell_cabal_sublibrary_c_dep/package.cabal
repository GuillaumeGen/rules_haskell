cabal-version:      2.4
name:               package
version:            0.1.0.0

executable package
    main-is:          Main.hs
    hs-source-dirs:   app
    default-language: Haskell2010
    build-depends:
        Cabal
        , base
        , package

library
    exposed-modules:
        Lib
    hs-source-dirs:
        lib
    build-depends:
        Cabal
        , base
        , sublib

library sublib
    exposed-modules:
        SubLib
    hs-source-dirs:
        sublib
    build-depends:
        Cabal
      , prettyprinter-compat-annotated-wl-pprint
      , base
    extra-libraries:
        z
        , ourclibrary
